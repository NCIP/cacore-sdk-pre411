<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.common.provenance.domain.impl">
  <class name="ProvenanceImpl" table="PROVENANCE" lazy="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="assigned" />
    </id>
    <property name="transformation" type="gov.nih.nci.common.util.StringClobType" column="TRANSFORMATION" />
    <property name="evidenceCode" type="java.lang.String" column="EVIDENCE_CODE" />
    <property name="objectIdentifier" type="java.lang.String" column="OBJECT_IDENTIFIER" />
    <property name="fullyQualifiedClassName" type="java.lang.String" column="FULLY_QUALIFIED_CLASS_NAME" />
    <property name="supplyingSourceId" type="java.lang.Long" column="SUPPLYING_SOURCE_ID" insert="false" update="false"/>
    <property name="originalSourceId" type="java.lang.Long" column="ORIGINAL_SOURCE_ID" insert="false" update="false"/>
    <property name="immediateSourceId" type="java.lang.Long" column="IMMEDIATE_SOURCE_ID" insert="false" update="false"/>
    <many-to-one name="supplyingSource" class="gov.nih.nci.common.provenance.domain.impl.SourceImpl" column="SUPPLYING_SOURCE_ID" lazy="true" />
    <many-to-one name="sourceReference" class="gov.nih.nci.common.provenance.domain.impl.SourceReferenceImpl" column="SOURCE_REFERENCE_ID" lazy="true" />
    <many-to-one name="immediateSource" class="gov.nih.nci.common.provenance.domain.impl.SourceImpl" column="IMMEDIATE_SOURCE_ID" lazy="true" />
    <many-to-one name="originalSource" class="gov.nih.nci.common.provenance.domain.impl.SourceImpl" column="ORIGINAL_SOURCE_ID" lazy="true" />
  </class>
</hibernate-mapping>

